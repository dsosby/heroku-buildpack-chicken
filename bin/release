#!/usr/bin/env bash
# bin/release <build-dir>

cat <<EOF
---
config_vars:
  PATH: /app/vendor/bin:/usr/local/bin:/usr/bin:/bin
  C_INCLUDE_PATH: /app/vendor/include:/app/vendor/include/chicken
  LD_LIBRARY_PATH: /app/vendor/lib:/app/vendor/lib/chicken/6
  CHICKEN_REPOSITORY: /app/vendor/lib/chicken/6
  CHICKEN_PREFIX: /app/vendor
default_process_types:
  web: csi -s run.scm
EOF
